{
	"manifest_version": 3,
	"name": "Twitter Lite Redirector",
	"version": "1.0.0",
	"description": "Twiter Lite（mobile.twitter.com）のリンクを通常のTwitter（twitter.com）へリダイレクトします。",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://mobile.twitter.com/*"
			],
			"js": [
				"./js/redirect.js"
			]
		}
	]
}
